                                    <option data-href="{{ path( app.request.get('_route', 'index'), app.request.get('_route_params', [])|merge({_locale: l.code})) }}" data-content='<span class="flag-icon flag-icon-{{ l.code }}"></span> {{ l.name }}'>{{ l.name }}
