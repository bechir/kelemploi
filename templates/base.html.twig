{# {% set currentPath = path(app.request.get('_route'), app.request.attributes.get('_route_params')) %} #}
{% set _locale = 'fr' %}

<!DOCTYPE html>
<html prefix="og: http://ogp.me/ns#" lang="fr" class="full-height" />
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no,shrink-to-fit=no" />
        <meta http-equiv="Content-Language" content="fr" />
        
        <meta name="application-name" content="{{ website.name }}" />
        <meta name="theme-color" content="white">

        <meta name="keywords" content="kelemploi, recrutement sénégal, recrutement dakar, emploi dakar, emploi senegal, concours, offre d'empoi, stage, stagiaire">

        <title>{% block title %}{{ website.name }}{% endblock %}</title>

        <meta property="og:locale" content="fr" />
        <meta property="og:type" content="website" />
        <meta property="og:site_name" content="{{ website.name }}" />
        <meta name="twitter:site" content="@{{ website.name }}" />
        {% block og_meta %}
        <meta property="og:title" content="{{ website.name }} Offres d'emploi et de stages au Sénégal" />
        <meta property="title" content="{{ website.name }} Offres d'emploi et de stages au Sénégal" />
        <meta property="twitter:card" content="summary" />
        <meta property="twitter:title" content="{{ website.name }} Offres d'emploi et de stages au Sénégal" />
        <meta property="twitter:image" content="{{ asset('images/logo.png') }}" />
        <meta name="description" content="" />
        <meta property="og:description" content="" />
        <meta property="twitter:description" content="" />
        {% endblock %}
        <link rel="manifest" href="{{ asset('manifest.json') }}">

        <!-- Bootstrap CSS -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet" />

        <!-- External Css -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.1.0/css/all.css" rel="stylesheet" />
        <link href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css" rel="stylesheet" />
        <link href="https://cdn.jsdelivr.net/npm/et-line@1.0.1/style.min.css" rel="stylesheet" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.11/css/bootstrap-select.min.css" rel="stylesheet" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/plyr/3.4.0/plyr.css" rel="stylesheet" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/0.8.0/css/flag-icon.min.css" rel="stylesheet" />
        <link href="{{ asset('assets/css/slick.css') }}" rel="stylesheet" />
        <link href="{{ asset('assets/css/jquery.nstSlider.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('assets/css/html5-simple-date-input-polyfill.css') }}" rel="stylesheet" />
        <link href="{{ asset('dashboard/css/dashboard.min.css') }}" rel="stylesheet" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" rel="stylesheet" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-tagsinput/0.6.1/bootstrap-tagsinput.min.css" rel="stylesheet" />

        <!-- Custom Css -->
        {% block stylesheets %}{% endblock %}
        <link href="{{ asset('css/main.min.css') }}" rel="stylesheet" type="text/css" />
        <!-- Fonts -->
        <link href="https://fonts.googleapis.com/css?family=Poppins:400,500,600%7CRoboto:300i,400,500" rel="stylesheet" />

        <!-- Favicon -->
        <link href="{{ asset('favicon.png') }}" rel="icon" />
        <link href="{{ asset('apple-touch-icon.png') }}" rel="apple-touch-icon" />
        <link href="{{ asset('icon-72x72.png') }}" rel="apple-touch-icon" sizes="72x72" />
        <link href="{{ asset('icon-114x114.png') }}" rel="apple-touch-icon" sizes="114x114" />

        <!--[if lt IE 9]>
            <script src="assets/js/html5shiv.min.js"></script>
            <script src="assets/js/respond.min.js"></script>
        <![endif]-->
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" type="image/x-icon" />

        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-153224488-1"></script>
        <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-153224488-1');
        </script>
    </head>
    <body>
    {# <div class="beta shadow-sm">Beta version</div> #}
    {% include 'common/flash_messages.twig' %}
    {% block body %}
        {% block header %}
            {% include 'common/header-2.html.twig' %}
        {% endblock header %}
        
        {% if not is_granted('ROLE_ADMIN') %}
            {% include "common/account-entry-modal.html.twig" %}
        {% endif %}

        <!--Main Layout-->
        <div class="transition-fade">
        {% block main %}{% endblock main %}
        </div>
        <!--Main Layout-->

        <!-- Footer -->
        {% block footer %}
            {# {% include 'common/footer.html.twig' %} #}
            {% include 'common/footer-2.html.twig' %}
        {% endblock footer %}
        <!-- Footer -->
        <script src="{{ asset('assets/js/intersection-observer.js') }}"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/js/bootstrap.bundle.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/feather-icons/4.7.3/feather.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.11/js/bootstrap-select.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-nstslider/1.0.13/jquery.nstSlider.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
        <script src="{{ asset('assets/js/visible.js') }}"></script>
        <script src="{{ asset('assets/js/jquery.countTo.js') }}"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/plyr/3.4.0/plyr.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/tinymce/4.6.4/tinymce.min.js"></script>
        <script src="{{ asset('assets/js/slick.min.js') }}"></script>
        <script src="{{ asset('assets/js/jquery.ajaxchimp.min.js') }}"></script>
        <script src="{{ asset('assets/js/html5-simple-date-input-polyfill.min.js') }}"></script>
        <script src="{{ asset('dashboard/js/dashboard.js') }}"></script>
        <script src="{{ asset('assets/js/typeahead.bundle.min.js') }}"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-tagsinput/0.6.1/bootstrap-tagsinput.min.js"></script>
        <script src="{{ asset('assets/js/bloodhound.min.js') }}"></script>
        <script>
            var user = {
                isAuth: {{ app.user ? 'true' : 'false' }},
            }
        </script>
        <script src="{{ asset('js/custom.js') }}"></script>
        {% block javascript %}{% endblock %}
    {% endblock %}

    </body>
</html>
